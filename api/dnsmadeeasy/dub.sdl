name "dnsmadeeasy"
description "A minimal D application."
copyright "Copyright Â© 2015, laeeth"
authors "laeeth"
dependency "kaleidic-auth" version="*"
dependency "kaleidic-helper" version="*"
configuration "dnsmadeeasy" {
	targetType "library"
